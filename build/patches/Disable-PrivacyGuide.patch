 BASE_FEATURE(kPrivacyGuidePreloadAndroid,
-             "PrivacyGuidePreloadAndroid",
-             base::FEATURE_DISABLED_BY_DEFAULT);
+             "PrivacyGuidePreloadAndroid",       // disabled
+             base::FEATURE_DISABLED_BY_DEFAULT); // by default
