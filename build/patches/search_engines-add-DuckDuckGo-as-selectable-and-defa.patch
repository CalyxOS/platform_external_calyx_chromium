     &yahoo,
+    &duckduckgo,
 };
 
 // Kuwait
@@ -611,6 +615,7 @@ const PrepopulatedEngine* const engines_MD[] = {
     &bing,
     &mail_ru,
     &yahoo,
+    &duckduckgo,
 };
 
 // Montenegro
@@ -881,6 +886,7 @@ const PrepopulatedEngine* const engines_TH[] = {
     &yahoo_th,
     &yandex_ru,
     &mail_ru,
+    &duckduckgo,
 };
 
